all: shell.out sb.out Assignment2_10_20CS30006_20CS30004_20CS10065_20CS30061.cpp squashbug_1.cpp

sb.out:
	g++ squashbug_1.cpp -o sb.out

shell.out:
	g++ Assignment2_10_20CS30006_20CS30004_20CS10065_20CS30061.cpp -lreadline -o shell.out

install_readline:
	apt install libreadline-dev

clean:
	-rm -f shell.out sb.out
